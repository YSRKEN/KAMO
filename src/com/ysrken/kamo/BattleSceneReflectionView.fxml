                           <ColumnConstraints />
                           <ColumnConstraints />
                       </columnConstraints>
                       <rowConstraints>
                           <RowConstraints />
                       </rowConstraints>
                       <ImageView fx:id="SceneImageView1" GridPane.rowIndex="0"/>
                       <Label fx:id="SceneLabel1" GridPane.rowIndex="1"/>
                   </GridPane>
               </content>
            </Tab>
            <Tab text="夜戦後">
              <content>
                  <GridPane>
                      <columnConstraints>
